    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/jokay03J-V2/w3c-validator/issues",
  "keywords": [
    "w3c",
    "validator",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  }
