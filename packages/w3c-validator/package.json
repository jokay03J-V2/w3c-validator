{
  "name": "@jokay03j-v2/w3c-validator",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "jokay03J @Jokay03J",
  "bin": {
    "w3c-validator": "./bin/run"
  },
  "homepage": "https://github.com/jokay03J-V2/w3c-validator",
  "license": "MIT",
  "repository": "jokay03J-V2/w3c-validator",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.11.8",
    "@oclif/plugin-help": "^5.2.17",
    "@oclif/plugin-plugins": "^3.2.6",
    "chalk": "^4.0.0",
    "w3c-validator-wrapper": "workspace:^"
  },
  "devDependencies": {
    "@oclif/test": "^2.4.5",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.41",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.11.3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "w3c-validator",
    "dirname": "w3c-validator",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
