{
  "name": "@jokay03j-v2/w3c-validator-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "wrapper and CLI for check file or url with W3C Markup Service",
  "author": "jokay03J-V2",
  "homepage": "https://github.com/jokay03J-V2/w3c-validator",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "jokay03J-V2/w3c-validator",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@oclif/test": "^3.0.0",
    "@types/chai": "^4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.18.34",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "chai": "^4",
    "eslint": "^8.42.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.0.0",
    "oclif": "^3",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "pnpm -r --filter='./packages/*' run build",
    "dev": "pnpm -r --parallel --filter='./packages/*' run dev",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "pnpm -r --filter='./packages/*' run test",
    "prettier:check": "prettier ./packages/**/*",
    "prettier:format": "prettier ./packages/**/* --write",
    "release": "pnpm run build && pnpm dlx @changesets/cli publish"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18.0.0 || >=20.0.0"
  },
  "bugs": "https://github.com/jokay03J-V2/w3c-validator/issues",
  "keywords": [
    "cli",
    "w3c",
    "validator"
  ],
  "files": ["*"]
}
