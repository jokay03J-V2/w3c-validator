{
  "name": "@jokay03j-v2/w3c-validator",
  "version": "2.1.2",
  "description": "wrapper and CLI for check file or url with W3C Markup Service",
  "author": "jokay03J-V2",
  "bin": {
    "w3c-validator": "./bin/run"
  },
  "homepage": "https://github.com/jokay03J-V2/w3c-validator",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "jokay03J-V2/w3c-validator",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@jokay03j-v2/w3c-validator": "^2.1.1",
    "@oclif/core": "^2",
    "@oclif/plugin-autocomplete": "^2.3.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.24",
    "@oclif/plugin-plugins": "^2.4.7",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "puppeteer": "^20.5.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.22",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.34",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "chai": "^4",
    "eslint": "^8.42.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.8.8",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "w3c-validator",
    "dirname": "w3c-validator",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "release": "npm run build && oclif manifest",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/jokay03J-V2/w3c-validator/issues",
  "keywords": [
    "cli",
    "w3c",
    "validator"
  ],
  "types": "dist/index.d.ts"
}
